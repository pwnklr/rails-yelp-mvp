<h1>hallo wiebke</h1>
<%= simple_form_for(@restaurant) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category, collection: Restaurant::CATEGORIES %>
  </div>

  <div class="form-actions btn btn-primary">
    <%= f.submit  %>
  </div>
<% end %>
